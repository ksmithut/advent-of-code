{
  "name": "advent-of-code",
  "version": "2.0.1",
  "description": "A cli to help initialize/run JavaScript advent-of-code challenges.",
  "main": "bin/advent.js",
  "scripts": {
    "lint": "standard",
    "test": "jest"
  },
  "bin": {
    "advent": "./bin/advent.js"
  },
  "files": [
    "bin",
    "lib"
  ],
  "keywords": [
    "adventofcode",
    "advent",
    "puzzle"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ksmithut/advent-of-code.git"
  },
  "author": "ksmithut",
  "license": "MIT",
  "eslintConfig": {
    "root": true,
    "extends": [
      "standard"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__fixtures__/"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branch": 100,
        "functions": 100,
        "lines": 100
      }
    }
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "commander": "^2.11.0",
    "configstore": "^3.1.1",
    "debug": "^3.1.0",
    "get-stdin": "^5.0.1",
    "mkdirp": "^0.5.1",
    "request": "^2.83.0",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "@types/jest": "^21.1.2",
    "jest": "^21.2.1",
    "nock": "^9.0.22",
    "rimraf": "^2.6.2",
    "standard": "^10.0.3"
  }
}
