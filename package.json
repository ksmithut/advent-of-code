{
  "name": "advent-of-code",
  "version": "4.0.2",
  "description": "A cli to help initialize/run JavaScript advent-of-code challenges.",
  "main": "bin/advent.js",
  "scripts": {
    "lint": "standard && prettier-eslint '**/*.js' --list-different",
    "test": "jest",
    "format": "prettier-eslint '**/*.js' --write"
  },
  "bin": {
    "advent": "./bin/advent.js"
  },
  "keywords": [
    "adventofcode",
    "advent",
    "puzzle"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ksmithut/advent-of-code.git"
  },
  "author": "ksmithut",
  "license": "MIT",
  "eslintConfig": {
    "root": true,
    "extends": [
      "standard"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__fixtures__/"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branch": 100,
        "functions": 100,
        "lines": 100
      }
    }
  },
  "dependencies": {
    "bluebird": "^3.5.5",
    "commander": "^2.20.0",
    "configstore": "^5.0.0",
    "debug": "^4.1.1",
    "get-stdin": "^7.0.0",
    "mkdirp": "^0.5.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.4"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "jest": "^24.8.0",
    "nock": "^10.0.6",
    "prettier-eslint-cli": "^5.0.0",
    "rimraf": "^2.6.3",
    "standard": "^13.0.0"
  }
}
